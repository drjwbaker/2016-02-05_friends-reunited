<script type="text/javascript">
	FR.Load(function() {
		BocaGrande.Pager.Setup({
			el: '.pager',
			url: 'http://www.friendsreunited.co.uk/Find?take=12&boxType=Town&Category=39&keywords=&start=&sort=Alphabetic',
			refreshUrl: 'http://www.friendsreunited.co.uk/Find?take=12',
			page: 1,
			take: 12,
			pageCount: 185,
			pagedElement: '#box-list-wrapper',
			size: 10,
			pagerMiddleIncrement: 5,
			sort: 'Alphabetic',
			scrollTarget: '#box-list-wrapper',
			data: {"boxType":10,"Category":39,"from":null,"to":null,"keywords":"","start":"","sort":"Alphabetic"},
			callback: function(result) {
				$('#gridView').masonry('destroy');
				$('#box-list-wrapper').html(result);
			    document.getElementById('contOuter').scrollIntoView();
				if (!$('body.mobile').length > 0) {
					$('#gridView').masonry({
						itemSelector: '.item',
						columnWidth: 1,
						cornerStampSelector: '.corner-stamp',
						isAnimated: true
					});
				} else {
					$('.corner-stamp').remove();
					//$('#gridView').masonry({
					//	itemSelector: '.item',
					//	columnWidth: 200
					//});
				}
			}
		});
	});
</script>
