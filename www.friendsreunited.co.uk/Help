        <div id="faq-search">   
           <form>
                <fieldset>
                    <table class="standardForm">
                        <tr class="field">
                            <td class="formCol1"><label>Search for a question:</label></td>
                            <td class="formCol2"><div class="inlineFields"><input type="text" id="searchTextBox" maxlength="200" /></div><input type="submit" class="primaryButton right" value="Search" id="searchButton" /></td>
                        </tr> 
