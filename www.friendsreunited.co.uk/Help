<script>
    FR.Load(function () {
        var FaqKey = 0;
        if (FaqKey != 0) {
            var data = {
                faq_key: FaqKey,
                userSearchText: ''
            };
            $('#faq-detail').html('<div id="loading"><img src="' + FR.StaticUrl + '/img/loadingSpinner.gif" alt="Loading" height="28px" width="28px" /></div>').show();

            FR.Data.Post("/Help/FaqDetail", { dataType: 'html' }, data, function (result) {
                $('#faq-detail').html(result);
                    $('#faq-detail p, #faq-detail span').removeAttr('style');
                    $('#faq-detail p').filter(function() {
                    	return $.trim($(this).text()) === '';
                    }).remove();
            });
        }

        $('#searchButton').live("click", function () {
            var data = {
                userSearchText: $('#searchTextBox').val()
            };
            $('#faq-detail').hide();
            $('#faq-search-results').html('<div id="loading"><img src="' + FR.StaticUrl + '/img/loadingSpinner.gif" alt="Loading" height="28px" width="28px" /></div>').show();
            FR.Data.Post("/Help/FaqSearch", { dataType: 'html' }, data, function (result) { 
                $('#faq-search-results').html(result);
            }, function () {
            	$('#faq-search-results').html('<div id="FAQsearch"><ul><li><h2>No results found for "' + data.userSearchText + '"</h2><ul></ul></li></ul></div>');
            });
            return false;
        });

        $('.faq-question').live("click", function () {
            $('#faqAnswer, #faq-detail').remove();
            $('.faq-question').removeClass('selected');
            $('.faq-question').show();
            //$(this).hide();
            $(this).addClass('selected');
            var searchText = $('#searchTextBox').val();
            var back = "<div><a href='#' class='backToResults'>Close</a></div>";
            //$('#faq-search-results').hide();
            $(this).parent().after("<li id='faqAnswer'><div id='faq-detail'></div></li>");
            $('#faq-detail').html('<div id="loading"><img src="' + FR.StaticUrl + '/img/loadingSpinner.gif" alt="Loading" height="28px" width="28px" /></div>').show();
            if (searchText == '') {
                var data = {
                    faq_key: $(this).attr("value")
                };

                FR.Data.Post("/Help/FaqDetail", { dataType: 'html' }, data, function (result) {
                    $('#faq-detail').html(result).append(back);
                    $('#faq-detail p, #faq-detail span').removeAttr('style');
                    $('#faq-detail p').filter(function() {
                    	return $.trim($(this).text()) === '';
                    }).remove();
                });

            } else {
                var data = {
                    faq_key: $(this).attr("value"),
                    userSearchText: $('#searchTextBox').val()
                };

                FR.Data.Post("/Help/FaqDetailSearch", { dataType: 'html' }, data, function (result) {
                    $('#faq-detail').html(result).show().append(back);
                    $('#faq-detail p, #faq-detail span').removeAttr('style');
                    $('#faq-detail p').filter(function() {
                        return $.trim($(this).text()) === ''
                    }).remove();
                });
            }
            return false;
        });

        $('.backToResults').live("click", function () {
            $('#faqAnswer').hide();
            $('.faq-question').show().removeClass('selected');
            return false;
        });

        $('.faq-header').live("click", function () {
           $('#faq-detail, .mobile #column1, .mobile #faq-search').hide();
           title= "<h2>" + $(this).text() + "</h2>";
           $('#faq-search-results').hide();
           $('#faq-search-results').html($(this).parent().parent().children('ul').clone()).prepend(title).show();
            return false;
        });

        $('.back-to-menu').live("click", function (e) {
            $('#faq-search-results').hide();
            $('#faq-menu').show();
            return false;
        });

        $('.resetFaq').live("click", function (e) {
            $('.mobile #column1, .mobile #faq-search').show();
            $('#faq-search-results').hide();
        });
    });
</script>
