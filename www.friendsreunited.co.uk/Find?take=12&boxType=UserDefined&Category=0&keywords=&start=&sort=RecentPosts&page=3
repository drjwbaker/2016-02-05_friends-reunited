<script type="text/javascript">
	FR.Load(function() {
		BocaGrande.Pager.Setup({
			el: '.pager',
			url: 'http://www.friendsreunited.co.uk/Find?take=12&boxType=UserDefined&Category=0&keywords=&start=&sort=RecentPosts',
			refreshUrl: 'http://www.friendsreunited.co.uk/Find?take=12',
			page: 3,
			take: 12,
			pageCount: 1998,
			pagedElement: '#box-list-wrapper',
			size: 10,
			pagerMiddleIncrement: 5,
			sort: 'RecentPosts',
			scrollTarget: '#box-list-wrapper',
			data: {"boxType":7,"Category":0,"from":null,"to":null,"keywords":"","start":"","sort":"RecentPosts"},
			callback: function(result) {
				$('#gridView').masonry('destroy');
				$('#box-list-wrapper').html(result);
			    document.getElementById('contOuter').scrollIntoView();
				if (!$('body.mobile').length > 0) {
					$('#gridView').masonry({
						itemSelector: '.item',
						columnWidth: 1,
						cornerStampSelector: '.corner-stamp',
						isAnimated: true
					});
				} else {
					$('.corner-stamp').remove();
					//$('#gridView').masonry({
					//	itemSelector: '.item',
					//	columnWidth: 200
					//});
				}
			}
		});
	});
</script>
